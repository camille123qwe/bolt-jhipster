<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Strategy.
    -->
    <changeSet id="20241118033533-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="execution_type_id"
                                 baseTableName="strategy"
                                 constraintName="fk_strategy__execution_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="execution_type"
                                 />

        <addForeignKeyConstraint baseColumnNames="source_id"
                                 baseTableName="strategy"
                                 constraintName="fk_strategy__source_id"
                                 referencedColumnNames="id"
                                 referencedTableName="source"
                                 />

        <addForeignKeyConstraint baseColumnNames="project_id"
                                 baseTableName="strategy"
                                 constraintName="fk_strategy__project_id"
                                 referencedColumnNames="id"
                                 referencedTableName="project"
                                 />
    </changeSet>
</databaseChangeLog>
